                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin></TextArea>
            <Button mnemonicParsing="false" onAction="#clearResults_Action" text="CLEAR" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
